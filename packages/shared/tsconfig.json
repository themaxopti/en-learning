{
  "compilerOptions": {
    "outDir": "./dist/",
    "baseUrl": ".",
    "noImplicitAny": true,
    "module": "ESNext",
    "target": "es5",
    "jsx": "react-jsx",
    "allowJs": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "composite": true,
    "paths": {
      "@services/*": [
        "../../services/*"
      ],
      "state/*": [
        "./src/state/*"
      ]
    }
  }
}